


<% if params[:step][:title] %>
	<% new_form = render :partial => "step_title", :locals => {:step => @step } %>
<% elsif params[:step][:description] %>
	<% new_form = render :partial => "step_description", :locals => {:step => @step } %>
<% end %>

var new_form = "<%= escape_javascript( new_form ) %>";